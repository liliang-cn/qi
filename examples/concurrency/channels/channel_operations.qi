包 主程序;

函数 入口() {
    // 测试 Channel 功能
    打印行("开始 Channel 测试");

    // 创建一个缓冲大小为3的通道
    变量 数据通道 = 通道<整数>(3);

    // 发送一些值到通道
    数据通道 <- 1;
    数据通道 <- 2;
    数据通道 <- 3;

    打印行("已发送3个值到通道");

    // 从通道接收值
    变量 值1 = <-数据通道;
    变量 值2 = <-数据通道;
    变量 值3 = <-数据通道;

    打印行("从通道接收到值: ", 值1);
    打印行("从通道接收到值: ", 值2);
    打印行("从通道接收到值: ", 值3);

    打印行("Channel 测试完成");
}