包 主程序;

// 定义一个简单的点类型
类型 点 {
    整数 x;
    整数 y;
}

// 返回 Future<点> 的简单函数
函数 创建点() : 未来<点> {
    变量 新点: 点 = (点 { x: 3, y: 4 });
    打印行("创建点 - x=3, y=4");
    返回 新点;
}

函数 入口() {
    打印行("测试开始");

    // 调用函数获取 Future
    变量 点未来: 未来<点> = 创建点();
    打印行("已调用创建点函数");

    // 等待 Future
    变量 结果: 点 = 等待 点未来;
    打印行("已等待Future");

    // 打印结果
    打印("结果.x = ");
    打印行(结果.x);
    打印("结果.y = ");
    打印行(结果.y);
}
