包 主程序;

// 综合测试 select 的所有情况类型

函数 入口() {
    打印行("=== Select 综合测试 ===");
    
    变量 通道1 = 通道<整数>();
    变量 通道2 = 通道<整数>();
    变量 发送值 = 100;
    
    // 测试1: 只有接收情况
    选择 {
        情况 <-通道1:
            打印行("从通道1接收（无赋值）");
        默认:
            打印行("测试1: 默认情况");
    }
    
    // 测试2: 带变量接收
    选择 {
        情况 数据 := <-通道1:
            打印行("从通道1接收到:", 数据);
        默认:
            打印行("测试2: 默认情况");
    }
    
    // 测试3: 发送情况
    选择 {
        情况 通道2 <- 发送值:
            打印行("发送到通道2:", 发送值);
        默认:
            打印行("测试3: 默认情况");
    }
    
    // 测试4: 混合多种情况
    选择 {
        情况 <-通道1:
            打印行("接收情况1");
        情况 值 := <-通道2:
            打印行("接收情况2，值:", 值);
        情况 通道1 <- 42:
            打印行("发送情况");
        默认:
            打印行("测试4: 默认情况");
    }
    
    打印行("=== Select 测试完成 ===");
}
