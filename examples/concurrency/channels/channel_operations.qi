包 主程序;

函数 入口() {
    // 测试 Channel 功能
    打印行("开始 Channel 测试");

    // 创建一个缓冲大小为3的通道
    变量 通道 = 通道<整数>(3);

    // 发送一些值到通道
    发送(通道, 1);
    发送(通道, 2);
    发送(通道, 3);

    打印行("已发送3个值到通道");

    // 从通道接收值
    变量 值1 = 接收(通道);
    变量 值2 = 接收(通道);
    变量 值3 = 接收(通道);

    打印行("从通道接收到值: " + 值1);
    打印行("从通道接收到值: " + 值2);
    打印行("从通道接收到值: " + 值3);

    // 关闭通道
    关闭通道(通道);

    打印行("Channel 测试完成");
}