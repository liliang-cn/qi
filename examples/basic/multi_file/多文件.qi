包 主程序;

导入 数学;

函数 入口() {
    打印("=== 多文件编译示例 ===");

    变量 数字1 = 10;
    变量 数字2 = 20;
    变量 和值 = 数字1 + 数字2;
    打印("主程序计算: 10 + 20 =", 和值);

    // 调用导入的函数
    变量 最大 = 最大值(数字1, 数字2);
    打印("最大值:", 最大);

    // 字符串测试
    变量 问候语 = "你好，世界！";
    打印("字符串输出:", 问候语);

    // 布尔值测试
    变量 条件 = 真;
    如果 条件 {
        打印("条件为真，执行了这个分支");
    }
}