// Qi编译器完整演示 | Qi Compiler Complete Demo

函数 显示标题() {
    打印 "=== Qi编译器功能演示 ===";
}

函数 测试变量() {
    变量 x = 100;
    变量 y = 50;
    变量 z = x + y;
    打印 "计算结果:";
    打印 z;
}

函数 测试运算() {
    变量 a = 15;
    变量 b = 3;
    打印 "加法:";
    打印 a + b;
    打印 "减法:";
    打印 a - b;
    打印 "乘法:";
    打印 a * b;
}

函数 主() {
    显示标题();
    测试变量();
    测试运算();
    打印 "演示完成！";
}