// Qi 语言演示程序
// 展示基本的语法特性

函数 主函数() {
    // 变量声明
    变量 整数值 = 42;
    变量 浮点值 = 3.14;
    变量 字符串值 = "你好，Qi！";
    变量 布尔值 = 真;
    
    // 算术运算
    变量 加法 = 10 + 20;
    变量 减法 = 30 - 15;
    变量 乘法 = 5 * 6;
    变量 除法 = 100 / 4;
    
    // 比较运算
    变量 大于 = 10 > 5;
    变量 小于 = 3 < 8;
    变量 等于 = 5 == 5;
    
    返回 0;
}
