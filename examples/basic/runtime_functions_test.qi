// Qi Runtime Functions Test
// 测试各种运行时函数的调用

函数 主函数() {
    // 字符串操作测试
    打印("=== 字符串操作测试 ===")
    
    变量 str1 = "你好"
    变量 str2 = "世界"
    
    // 字符串长度
    变量 len = 长度(str1)
    打印("字符串长度:")
    打印(len)
    
    // 字符串连接
    变量 result = 连接(str1, str2)
    打印("连接结果:")
    打印(result)
    
    // 数学运算测试
    打印("=== 数学运算测试 ===")
    
    // 平方根
    变量 sqrt_result = 平方根(16.0)
    打印("16的平方根:")
    打印(sqrt_result)
    
    // 幂运算
    变量 pow_result = 次方(2.0, 3.0)
    打印("2的3次方:")
    打印(pow_result)
    
    // 绝对值
    变量 abs_result = 绝对值(-42)
    打印("|-42|:")
    打印(abs_result)
    
    // 四舍五入
    变量 round_result = 四舍五入(3.7)
    打印("3.7四舍五入:")
    打印(round_result)
    
    // 类型转换测试
    打印("=== 类型转换测试 ===")
    
    变量 num = 42
    变量 num_str = 整数转字符串(num)
    打印("整数转字符串:")
    打印(num_str)
    
    变量 float_num = 3.14
    变量 float_str = 浮点数转字符串(float_num)
    打印("浮点数转字符串:")
    打印(float_str)
    
    变量 str_num = "123"
    变量 parsed_int = 字符串转整数(str_num)
    打印("字符串转整数:")
    打印(parsed_int)
    
    打印("=== 测试完成 ===")
}
