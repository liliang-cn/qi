// Qi Runtime Functions Test
// 测试各种运行时函数的调用

函数 主函数() {
    // 字符串操作测试
    变量 message1 = "=== 字符串操作测试 ===";

    变量 str1 = "你好";
    变量 str2 = "世界";

    // 字符串连接
    变量 result = str1 + str2;

    // 数学运算测试
    变量 message2 = "=== 数学运算测试 ===";

    // 简单数学运算
    变量 sum = 16 + 9;
    变量 product = 2 * 3;
    变量 diff = 42 - 100;
    变量 quotient = 14 / 2;

    // 类型测试
    变量 message3 = "=== 类型测试 ===";

    变量 num = 42;
    变量 float_num = 3.14;
    变量 str_num = "123";

    // 条件测试
    如果 num > 0 {
        变量 positive = true;
    } 否则 {
        变量 positive = false;
    }

    // 循环测试
    变量 i = 0;
    变量 counter = 0;
    当 i < 5 {
        counter = counter + 1;
        i = i + 1;
    }

    变量 completion_message = "=== 测试完成 ===";
}
